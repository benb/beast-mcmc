	<traceAnalysis fileName="testRelaxedClock.log">
		<expectation name="likelihood" value="-3843.57"/>
		<expectation name="mean" value="-7.03510"/>
		<expectation name="stdev" value="0.24720"/>
		<expectation name="meanRate" value="9.13844E-4"/>
		<expectation name="covariance" value="-3.40794E-2"/>
		<expectation name="siteModel.shape" value="5.33912E-2"/>
		<expectation name="demo1.popSize" value="38.2230"/>
		<expectation name="hky.kappa" value="21.6729"/>
		<expectation name="treeModel.rootHeight" value="75.2975"/>
		<expectation name="treeLikelihood" value="-3771.19"/>
