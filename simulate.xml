<?xml version="1.0" standalone="yes"?>
<beast>
    <taxa id="taxa">
        <taxon id="A"/>
        <taxon id="B"/>
        <taxon id="C"/>
        <taxon id="D"/>
        <taxon id="E"/>
        <taxon id="F"/>
    </taxa>

      <constantSize id="constant" units="years">
        <populationSize>
            <parameter id="constant.popSize" value="10.0" lower="0.0"/>
        </populationSize>
    </constantSize>

    <!-- Generate a random starting tree under the coalescent process            -->
    <coalescentTree id="startTree">
        <taxa idref="taxa"/>
        <constantSize idref="constant"/>
    </coalescentTree>

    <report>
        <coalescentTree idref="startTree"/>
    </report>

</beast>
